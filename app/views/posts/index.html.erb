<table class="table table-hover">
  <thead>
    <tr class="table_header">
      <th>Recent Shibes Barking</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |p| %>
      <tr>
        <td style="width:100%"><%= link_to p.title, post_path(p) %> <%= p.created_at.strftime("%b %d, %Y")%> <%= p.user.email %></td>
        </td>
      </tr>
      <% end %>
  </tbody>
</table>
<%= paginate @posts %>
<div class="row">
  <div class="col-lg-4 col-sm-6 col-xs-12">
    <img src="assets/harry_doge.jpg" class="thumbnail img-responsive">
  </div>
  <div class="col-lg-4 col-sm-6 col-xs-12">
    <img src="assets/flat_doge.png" class="thumbnail img-responsive">
  </div>
  <div class="col-lg-4 col-sm-6 col-xs-12">
    <img src="assets/roman_doge.png" class="thumbnail img-responsive">
  </div>
  <div class="col-lg-4 col-sm-6 col-xs-12">
    <img src="assets/windoge.jpg" class="thumbnail img-responsive">
  </div>
  <div class="col-lg-4 col-sm-6 col-xs-12">
    <img src="assets/nature_doge.jpg" class="thumbnail img-responsive">
  </div>
  <div class="col-lg-4 col-sm-6 col-xs-12">
    <img src="assets/talentdoge.jpg" class="thumbnail img-responsive">
  </div>
</div>
